{
    "name": "fsi/admin-gallery-bundle",
    "type": "symfony-bundle",
    "description": "FSiAdminBundle configuration for fsi/gallery-bundle",
    "keywords": ["gallery", "photo", "symfony2", "bundle", "fsi", "admin"],
    "license": "MIT",
    "authors": [
        {
            "name": "Norbert Orzechowicz",
            "email": "norbert@orzechowicz.pl"
        }
    ],
    "require": {
        "php": ">=5.4",
        "doctrine/orm": "^2.3",
        "doctrine/doctrine-bundle": "dev-master",
        "symfony/framework-bundle": "^2.3",
        "symfony/twig-bundle": "^2.3",
        "egeloen/ckeditor-bundle": "^2.0",
        "fsi/admin-bundle" : "~1.1@dev",
        "fsi/gallery-bundle": "1.1.*"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.3",
        "symfony/validator": "^2.3",
        "phpspec/phpspec": "^2.4",
        "behat/behat": "^2.4",
        "behat/symfony2-extension": "^1.1",
        "behat/mink": "^1.7",
        "behat/mink-extension": "^1.3",
        "behat/mink-browserkit-driver": "^1.3",
        "behat/mink-selenium2-driver": "^1.3",
        "sensiolabs/behat-page-object-extension": "^1.0",
        "bossa/phpspec2-expect": "^1.0"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-4": {
            "FSi\\Bundle\\AdminGalleryBundle\\": ""
        }
    },
    "target-dir" : "FSi/Bundle/AdminGalleryBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    }
}
